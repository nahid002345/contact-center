      <dependency id="Newtonsoft.Json" version="4.5.11" />
      <dependency id="jQuery" version="1.8.2" />
      <dependency id="jQuery.Validation" version="1.10.0" />
      <dependency id="Microsoft.jQuery.Unobtrusive.Validation" version="3.1.1" />
    </dependencies>
  </metadata>
</package>